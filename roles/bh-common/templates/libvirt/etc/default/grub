GRUB_TIMEOUT=5
GRUB_DEFAULT=saved
GRUB_DISABLE_SUBMENU=true
GRUB_TERMINAL_OUTPUT="serial"
GRUB_SERIAL_COMMAND="serial --speed=115200 --unit=0 --word=8 --parity=no --stop=1"
GRUB_CMDLINE_LINUX="crashkernel=auto rd.lvm.lv=vg00/root_lv rd.lvm.lv=vg00/swap_lv rd.lvm.lv=vg00/usr_lv console=ttyS0,115200"
GRUB_DISABLE_RECOVERY="true"

